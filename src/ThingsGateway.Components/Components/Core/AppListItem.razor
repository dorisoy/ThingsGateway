

@namespace ThingsGateway.Components
@using Masa.Blazor
 
<MListItem ActiveClass="primary--text" Href="@Item?.Href" Link Target="@Item?.Target"
           Exact>
    @if (Item?.Icon is not null)
    {
        <MListItemIcon>
            <MIcon>@Item.Icon</MIcon>
        </MListItemIcon>
    }

    <AppListItemContent Item="Item" />

</MListItem>

@code {

    [Parameter, EditorRequired]
    public NavItem Item { get; set; }
}
