@page "_Host"
@using Microsoft.AspNetCore.Components.Web;
@using ThingsGateway.Components;
@using ThingsGateway.Demo;
@using Microsoft.AspNetCore.Authorization;
@attribute [AllowAnonymous]
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, viewport-fit=cover" />
    <title>ThingsGateway</title>
    <base href="/" />

    <link href="_content/Masa.Blazor/css/masa-blazor.min.css" rel="stylesheet" />
    <link href=@(BlazorAppService.DefaultResourceUrl+"css/materialdesign/v7.1.96/css/materialdesignicons.min.css") rel="stylesheet">
    <link href=@(BlazorAppService.DefaultResourceUrl+"css/material/icons.css") rel="stylesheet">
    <link href=@(BlazorAppService.DefaultResourceUrl+"css/fontawesome/v6.4.0/css/all.min.css") rel="stylesheet">
    <link href=@(BlazorAppService.DefaultResourceUrl+"style/common.css") rel="stylesheet">
    <link href=@(BlazorAppService.DefaultResourceUrl+"prism/prism-material-dark-for-masa.css") rel="stylesheet">
    <link href=@(BlazorAppService.DefaultResourceUrl+"prism/prism-line-highlight.min.css") rel="stylesheet">
    <!-- PWA Manifest -->
    <link rel="manifest" href="./manifest.json" />

</head>


<body>

    <app>
        <component type="typeof(App)" render-mode="Server" />
    </app>

    <script src="_framework/blazor.server.js"></script>
    <script src=@(BlazorAppService.DefaultResourceUrl+"js/jquery-3.5.1.js") type="text/javascript" charset="utf-8"></script>
    <script src="_content/BlazorComponent/js/blazor-component.js"></script>
    <script src=@(BlazorAppService.DefaultResourceUrl+"prism/prism.min.js")></script>
    <script src=@(BlazorAppService.DefaultResourceUrl+"echarts/5.4.2/echarts.min.js") charset="utf-8"></script>
    <script src=@(BlazorAppService.DefaultResourceUrl+"echarts/theme/vintage.js") charset="utf-8"></script>
    <!-- PWA Service Worker -->
    <script type="text/javascript">'serviceWorker' in navigator && navigator.serviceWorker.register('./service-worker.js')</script>

</body>

</html>
