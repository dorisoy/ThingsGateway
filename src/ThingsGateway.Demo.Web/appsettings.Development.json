{
  "Urls": "http://0.0.0.0:7200", //监听地址

  "AllowedHosts": "*",

  "DetailedErrors": true,

  //日志配置
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "EventLog": {
      "LogLevel": {
        "Default": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      } //windows事件输出日志等级
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      } //控制台输出日志等级
    }
  }
}